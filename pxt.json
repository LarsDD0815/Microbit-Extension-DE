{
    "name": "Microbit-Bluetooth",
    "version": "1.0.0",
    "description": "Microbit mit Bluetooth-Controller",
    "dependencies": {
        "core": "*",
        "neopixel": "github:mworkfun/pxt-neopixel-bluetooth",
        "microphone": "*",
        "bluetooth": "*"
    },
    "files": [
        "README.md",
        "main.ts"
    ],
    "testFiles": [
        "test.ts"
    ],
    "public": true,
    "targetVersions": {
        "target": "5.0.6",
        "targetId": "microbit"
    },
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "open": 1,
                    "whitelist": 0,
                    "security_level": null
                }
            }
        }
    }
}
